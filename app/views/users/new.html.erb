<%= form_for @user, html: {class: 'form-signin'} do |form| %>
  <h2><%= t(".title") %></h2>
  <%= render partial: '/users/form', object: form %>
  <%= form.submit class: 'btn btn-lg btn-primary btn-block' %>
  <div class="other-links">
    <%= link_to t(".sign_in"), sign_in_path %>
  </div>
<% end %>

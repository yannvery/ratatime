<%= simple_form_for @tracker do |f| %>
  <%= f.input :logged_date, input_html: { class: "datepicker" } %>
  <%= f.input :duration %>
  <%= f.association :project, collection: current_user.projects.order(:name), prompt: "Choose a Project" %>
  <%= f.input :description %>
  <%= f.button :submit %>
  <%= link_to "Cancel", :back, class: "modal-action modal-close waves-effect waves-green btn-flat"%>
<% end %>
